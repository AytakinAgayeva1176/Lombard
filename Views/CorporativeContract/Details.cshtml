@model Lombard.Domain.ViewModels.CorporativeContractDetailsVM

@{
    ViewData["Title"] = "Details";
}

<!-- Form holder starts here -->
<div class="main-form-holder-inside-page-cont d-f">
    <div class="label-holder-inside-form-holder d-f">
        <p class="f-s-24 f-w-bold"
           id="PageIndicator" data-page="Korporativ müqavilə" data-indicator="Korporativ müqavilənin detallarına baxırsınız">Korporativ müqavilənin detalları</p>
    </div>

    <div class="form-part-inside-form-holder d-f">
        <div class="label-row-inside-form-part d-f">
            <p class="f-s-18 f-w-bold">Müştəri</p>
        </div>
        <div class="input-row-inside-form-part d-g">
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Adı, Soyadı , Ata adı</label>
                @if (Model.LegalDebtorId != null)
                {
                    <input type="text" value="@Html.DisplayFor(model => model.LegalDebtor.Name)" readonly>
                }
                else if (Model.IndividualDebtorId != null)
                {
                    <input type="text" value="@Html.DisplayFor(model => model.IndividualDebtor.Name) @Html.DisplayFor(model => model.IndividualDebtor.Surname) @Html.DisplayFor(model => model.IndividualDebtor.Patronymic)" readonly>
                }
            </div>

            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Hesab nömrəsi</label>
                <input type="text" value="@Html.DisplayFor(model => model.AccountNumber)" readonly>
            </div>
        </div>
    </div>
    <div class="form-part-inside-form-holder d-f">
        <div class="label-row-inside-form-part d-f">
            <p class="f-s-18 f-w-bold">Müqavilə məlumatları</p>
        </div>
        <div class="input-row-inside-form-part d-g">
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Müqavilənin nömrəsi </label>
                <input type="text" value="@Html.DisplayFor(model => model.ContractNo)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Valyuta</label>
                <input type="text" value="@Html.DisplayFor(model => model.Currency)" readonly>
            </div>
        </div>
    </div>
    <div class="form-part-inside-form-holder d-f">
        <div class="label-row-inside-form-part d-f">
            <p class="f-s-18 f-w-bold">Kredit məlumatları</p>
        </div>
        <div class="input-row-inside-form-part d-g">
        
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Məhsul adı</label>
                <input type="text" value="@Html.DisplayFor(model => model.Product.Name)" readonly>
            </div>

            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Verilmə tarixi</label>
                <input class="date_input_for_details" type="text" value="@Html.DisplayFor(model => model.GivingTime)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Müddət</label>
                <input type="text" value="@Html.DisplayFor(model => model.Period)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Ödəniş tarixi</label>
                <input type="text" value="@Html.DisplayFor(model => model.PaymentDate)" readonly>
            </div>

            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Güzəşt ayları</label>
                <input type="text" value="@Html.DisplayFor(model => model.DiscountedMonths)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Komissiya</label>
                <input type="text" value="@Html.DisplayFor(model => model.Comission)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Faiz dərəcəsi</label>
                <input type="text" value="@Html.DisplayFor(model => model.Percentage)" readonly>
            </div>

            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">FIFD</label>
                <input type="text" value="@Html.DisplayFor(model => model.FIFD)" readonly>
            </div>
            @if (Model.ParentId != null)
            {
                <div class="input-holder-with-label d-f">
                    <label for="customInput" class="f-s-14 f-w-500">Kreditin məbləği</label>
                    <input type="text" value="@Html.DisplayFor(model => model.CreditAmount)" readonly>
                </div>
            }
            else
            {
                <div class="input-holder-with-label d-f">
                    <label for="customInput" class="f-s-14 f-w-500">Kreditin Limiti</label>
                    <input type="text" value="@Html.DisplayFor(model => model.CreditLimit)" readonly>
                </div>

                <div class="form-part-inside-form-holder d-f">
                    <div class="input-row-inside-form-part d-g">
                        <div class="input-holder-with-label d-f">
                            <label for="DebtorId" class="f-s-14 f-w-500">Zamin</label>
                            <a asp-action="GuarantorDetails" asp-route-id="@ViewData["guarantorId"]" asp-route-corporativContractId="@Model.Id">
                                <input type="text" value="@ViewData["Guarantor"]" readonly class="t-d-n crsr-pnter link-input">
                            </a>
                        </div>
                    </div>
                </div>
            }

        </div>
    </div>


    <div class="button-holder-inside-form-holder d-f f-center">
        <a asp-action="Edit" asp-route-id="@Model.Id" asp-route-returnUrl="/CorporativeContract/Details/@Model.Id" class="green-button d-f t-d-n g-10 align-center">
            Dəyiş
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path class="stroke-color-change-green" d="M13 11L21.2 2.80005" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                <path class="stroke-color-change-green" d="M22 6.8V2H17.2" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
                <path class="stroke-color-change-green" d="M11 2H9C4 2 2 4 2 9V15C2 20 4 22 9 22H15C20 22 22 20 22 15V13" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round" />
            </svg>
        </a>
    </div>
</div>
