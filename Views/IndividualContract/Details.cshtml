    <div class="form-part-inside-form-holder d-f">
        <div class="label-row-inside-form-part d-f">
            <p class="f-s-18 f-w-bold">Müştəri</p>
        </div>
        <div class="input-row-inside-form-part d-g">
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Adı, Soyadı , Ata adı</label>
                @if (Model.IndividualContract.LegalDebtorId != null)
                {
                    <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.LegalDebtor.Name)" readonly>
                }
                else if (Model.IndividualContract.IndividualDebtorId != null)
                {
                    <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.IndividualDebtor.Name) @Html.DisplayFor(model => model.IndividualContract.IndividualDebtor.Surname) @Html.DisplayFor(model => model.IndividualContract.IndividualDebtor.Patronymic)" readonly>
                }
            </div>

            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Hesab nömrəsi</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.AccountNumber)" readonly>
            </div>
        </div>
    </div>
    <div class="form-part-inside-form-holder d-f">
        <div class="label-row-inside-form-part d-f">
            <p class="f-s-18 f-w-bold">Müqavilə məlumatları</p>
        </div>
        <div class="input-row-inside-form-part d-g">
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Müqavilənin nömrəsi </label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.ContractNo)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Valyuta</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.Currency)" readonly>
            </div>
        </div>
    </div>
    <div class="form-part-inside-form-holder d-f">
        <div class="label-row-inside-form-part d-f">
            <p class="f-s-18 f-w-bold">Kredit məlumatları</p>
        </div>
        <div class="input-row-inside-form-part d-g">

            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Məhsul adı</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.Product.Name)" readonly>
            </div>

            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Verilmə tarixi</label>
                <input class="date_input_for_details" type="text" value="@Html.DisplayFor(model => model.IndividualContract.GivingTime)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Müddət</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.Period)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Ödəniş tarixi</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.PaymentDate)" readonly>
            </div>

            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Güzəşt ayları</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.DiscountedMonths)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Komissiya</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.Comission)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Faiz dərəcəsi</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.Percentage)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">FIFD</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.FIFD)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Kreditin məbləği</label>
                <input type="text" value="@Html.DisplayFor(model => model.IndividualContract.CreditAmount)" readonly>
            </div>
        </div>
    </div>
    <div class="form-part-inside-form-holder d-f">
        <div class="input-row-inside-form-part d-g">
            <div class="input-holder-with-label d-f">
                <label for="DebtorId" class="f-s-14 f-w-500">Zamin</label>
                <a asp-action="GuarantorDetails" asp-route-id="@ViewData["guarantorId"]">
                    <input type="text" value="@ViewData["Guarantor"]" readonly class="t-d-n crsr-pnter link-input">
                </a>
