@model Lombard.Domain.ViewModels.IndividualDebtorWithALLInfo

@{
    ViewData["Title"] = "Details";
}


<!-- Form holder starts here -->individualDebtorWithALLInfo
<div class="main-form-holder-inside-page-cont d-f">
    <div class="label-holder-inside-form-holder d-f">
        <p class="f-s-24 f-w-bold" 
        id="PageIndicator" data-page="Fiziki borcalan" data-indicator="Detallar səhifəsinə baxırsınız">
            Fiziki Borcalan
        </p>
    </div>
    <div class="form-part-inside-form-holder d-f">
        <div class="label-row-inside-form-part d-f">
            <p class="f-s-18 f-w-bold">Şəxsi məlumatlar</p>
        </div>
        <div class="input-row-inside-form-part d-g">
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Ad</label>
                <input type="text" value="@Html.DisplayFor(model => model.Name)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput2" class="f-s-14 f-w-500">Soyad</label>
                <input type="text" value="@Html.DisplayFor(model => model.Surname)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput3" class="f-s-14 f-w-500">Ata adı</label>
                <input type="text" value="@Html.DisplayFor(model => model.Patronymic)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput4" class="f-s-14 f-w-500">Doğum tarixi</label>
                <input class="date_input_for_details" type="text" value="@Html.DisplayFor(model => model.BirthDate)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput5" class="f-s-14 f-w-500">Doğulduğu yer</label>
                <input type="text" value="@Html.DisplayFor(model => model.BirthPlace)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput6" class="f-s-14 f-w-500">Faktiki yaşayış ünvanı</label>
                <input type="text" value="@Html.DisplayFor(model => model.CurrentLivingAddress)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput6" class="f-s-14 f-w-500">Qeydiyyat ünvanı</label>
                <input type="text" value="@Html.DisplayFor(model => model.RegisteredAddress)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput6" class="f-s-14 f-w-500">Email</label>
                <input type="text" value="@Html.DisplayFor(model => model.Email)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput6" class="f-s-14 f-w-500">Cinsi</label>
                <input type="text" value="@Html.DisplayFor(model => model.Gender)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput6" class="f-s-14 f-w-500">Ailə vəziyyəti</label>
                <input type="text" value="@Html.DisplayFor(model => model.MarialStatuses)" readonly>
            </div>
        </div>
    </div>
    <div class="form-part-inside-form-holder d-f">
        <div class="label-row-inside-form-part d-f">
            <p class="f-s-18 f-w-bold">Şəxsiyyət vəsiqəsi məlumatları</p>
        </div>
        <div class="input-row-inside-form-part d-g">
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Şəxsiyyət vəsiqəsi FİN kodu</label>
                <input type="text" value="@Html.DisplayFor(model => model.IDFincode)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput2" class="f-s-14 f-w-500">Şəxsiyyət vəsiqəsi seriya nömrəsi</label>
                <input type="text" value="@Html.DisplayFor(model => model.IDSerialNumber)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput3" class="f-s-14 f-w-500">Vəsiqəni verən orqan</label>
                <input type="text" value="@Html.DisplayFor(model => model.IDOrganizationName)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput4" class="f-s-14 f-w-500">Vəsiqənin verilmə tarixi </label>
                <input class="date_input_for_details" type="text" value="@Html.DisplayFor(model => model.IDGiveDate)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput5" class="f-s-14 f-w-500">Vəsiqənin etbarlılıq müddəti</label>
                <input class="date_input_for_details" type="text" value="@Html.DisplayFor(model => model.IDExpireDate)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput6" class="f-s-14 f-w-500">Vətəndaşlıq</label>
                <input type="text" value="@Html.DisplayFor(model => model.Citizenship)" readonly>
            </div>
        </div>
    </div>
    <div class="form-part-inside-form-holder d-f">
        <div class="label-row-inside-form-part d-f">
            <p class="f-s-18 f-w-bold">Əlaqə məlumatları</p>
        </div>
        <div class="input-row-inside-form-part d-g">
            <div class="input-holder-with-label d-f">
                <label for="customInput" class="f-s-14 f-w-500">Mobil telefonu</label>
                <input type="text" value="@Html.DisplayFor(model => model.MobileNumber)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput2" class="f-s-14 f-w-500">Ev telefonu</label>
                <input type="text" value="@Html.DisplayFor(model => model.Phone)" readonly>
            </div>
            <div class="input-holder-with-label d-f">
                <label for="customInput3" class="f-s-14 f-w-500">Əlavə telefon</label>
                <input type="text" value="@Html.DisplayFor(model => model.PhoneAdditional)" readonly>
            </div>
        </div>
    </div>
    <div class="button-holder-inside-form-holder d-f f-center">
        <a asp-action="Edit" asp-route-id="@Model.Id" asp-route-returnUrl="/IndividualDebtor/Details/@Model.Id" class="green-button d-f t-d-n g-10 align-center">
            Dəyiş
            <svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path class="stroke-color-change-green" d="M13 11L21.2 2.80005" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path class="stroke-color-change-green" d="M22 6.8V2H17.2" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                <path class="stroke-color-change-green" d="M11 2H9C4 2 2 4 2 9V15C2 20 4 22 9 22H15C20 22 22 20 22 15V13" stroke="white" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
            </svg>
        </a>
    </div>
</div>
<!-- Form holder ends here -->




